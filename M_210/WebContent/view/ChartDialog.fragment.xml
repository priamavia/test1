<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:viz="sap.viz.ui5.controls" 
	xmlns:core="sap.ui.core">
	<Dialog
	       resizable="true" 
	        contentWidth="80rem" 
	        contentHeight="40rem"
	        draggable = "true"
   			title="{i18n>chart_title}">
		<content>
	    
	<!--   Chart 출력 로직  -->
		<viz:VizFrame xmlns="sap.viz" id="idcolumn" width="100%">
		</viz:VizFrame>
		
		</content>
		<beginButton>
			<Button
				text="{i18n>close}"
				press="onCloseDialog"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>